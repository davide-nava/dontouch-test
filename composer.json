{
    "version": "1.0.0",
    "name": "davide-nava/donthouch-test",
    "description": "Test api profiles.",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "authors": [
        {
            "name": "Davide Nava",
            "email": "davidenava85@gmail.com"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.3",
        "darkaonline/l5-swagger": "^8.6",
        "doctrine/annotations": "^2.0",
        "illuminate/collections": "9.*",
        "illuminate/contracts": "9.*",
        "illuminate/filesystem": "9.*",
        "illuminate/macroable": "9.*",
        "illuminate/support": "9.*",
        "illuminate/translation": "9.*",
        "laravel/lumen-framework": "9.*",
        "laravel/ui": "^4.5",
        "monolog/monolog": "*",
        "nextapps/laravel-swagger-ui": "^0.10.1",
        "predis/predis": "^2.2",
        "tymon/jwt-auth": "*",
        "zircote/swagger-php": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
